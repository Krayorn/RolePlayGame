{
  "env": {
    "browser": true,
    "node": true
  },
  "extends": [
    "react-app"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true,
      "jsx": true
    },
    "sourceType": "module"
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "quotes": [ 2, "single", {"allowTemplateLiterals": true} ],
    "camelcase": "off",
    "indent": [
      "error",
      2,
      {
        "SwitchCase": 1
      }
    ],
    "no-multiple-empty-lines": "error",
    "no-console": "warn",
    "no-var": "error",
    "prefer-const": "error",
    "no-empty": "error",
    "no-unused-vars": "error",
    "semi": [2, "never"],
    "no-trailing-spaces": "error",
    "eol-last": ["error", "always"]
  },
  "globals": {
    "ga": true,
    "places": true
  }
}
